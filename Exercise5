#include <iostream>
#include <cmath>
using namespace std;

double distance(double X1,double a, double Y1,double b){
    double x=pow((X1-a),2);
    double y=pow((Y1-b),2);
    double ans=x+y;
    double answer=sqrt(ans);
    return answer;
}
int main(){

    double X1,a,Y1,b;
    cout<<"Enter the value of X1"<<endl;
    cin>>X1;
    cout<<"Enter the value of X2"<<endl;
    cin>>a;
    cout<<"Enter the value of Y1"<<endl;
    cin>>Y1;
    cout<<"Enter the value of Y2"<<endl;
    cin>>b;
    
    double answer=distance(X1,a,Y1,b);
    cout<<"The bistance is :"<<answer<<endl;
}
